<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.adminax.engine.mapper.ConfigMapper">

    <select id="findByKey" resultType="com.adminax.engine.dto.DevConfigDTO">
        SELECT 
            config_key, 
            config_value, 
            description 
        FROM dev_config 
        WHERE config_key = #{configKey}
    </select>

</mapper>